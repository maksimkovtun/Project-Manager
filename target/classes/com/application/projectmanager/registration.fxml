<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="202.0" prefWidth="189.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.application.projectmanager.Controllers.registrationController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="168.0" prefWidth="167.0">
        <children>
            <Button alignment="CENTER" layoutX="44.0" layoutY="143.0" onAction="#onRegButtonClick" text="REGISTER" />
            <Label id="loginLabel" alignment="CENTER" layoutX="46.0" layoutY="23.0" style="-fx-font-weight: bold;" text="USERNAME" />
            <TextField fx:id="loginTextField" alignment="CENTER" layoutX="1.0" layoutY="46.0" />
            <Label id="passwordLabel" alignment="CENTER" layoutX="45.0" layoutY="80.0" style="-fx-font-weight: bold;" text="PASSWORD" />
            <PasswordField fx:id="passwordTextField" alignment="CENTER" layoutX="2.0" layoutY="104.0" />
            <Label fx:id="infoLabel" alignment="CENTER" layoutX="6.0" layoutY="6.0" prefHeight="17.0" prefWidth="139.0" style="-fx-text-fill: red;" textAlignment="CENTER" />
            <Hyperlink layoutX="3.0" layoutY="-11.0" style="-fx-font-weight: bold;" text="&lt;--" onAction="#onAuthLinkClick"/>
        </children>
    </AnchorPane>
</VBox>
